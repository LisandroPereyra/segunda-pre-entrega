<h1>Productos</h1>

<table id="main"> </table>

<tr>
    <th>id</th>
    <th>tittle</th>
    <th>description</th>
    <th>price</th>
    <th>thumbnail</th>
    <th>code</th>
    <th>stock</th>
    {{#each products}}
    <tr>
        <th>{{this.id}}</th>
        <th>{{this.tittle}}</th>
        <th>{{this.description}}</th>
        <th>{{this.price}}</th>
        <th>{{this.thumbnail}}</th>
        <th>{{this.code}}</th>
        <th>{{this.stock}}</th>
        <button onclick=addToCart(this) id={{_id}}>Add to cart</button>
    </tr>
    {{#each}}
</tr>



<script src="{{socket}}"></script>
<script src="{{index}}"></script>